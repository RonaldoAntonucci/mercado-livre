{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "rootDirs": [
      "src",
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "emitDecoratorMetadata": true
  },
  "include": [
    "src",
  ],
  "exclude": []
}
